const express = require("express");

const router = express.Router();

router.get("/dashboard/stats", async (_req, res) => {
  try {
    res.json({
      success: true,
      data: {
        currentMonthRevenue: 0,
        lastMonthRevenue: 0,
        topMedicines: [],
      },
    });
  } catch (_err) {
    res.status(500).json({ success: false, message: "Dashboard error" });
  }
});

module.exports = router;
